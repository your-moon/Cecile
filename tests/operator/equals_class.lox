// Bound methods have identity equality.
type Foo;
type Bar;

println Foo == Foo; // out: true
println Foo == Bar; // out: false
println Bar == Foo; // out: false
println Bar == Bar; // out: true

println Foo == "Foo"; // out: false
println Foo == nil;   // out: false
println Foo == 123;   // out: false
println Foo == true;  // out: false
